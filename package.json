{
  "name": "@projectwallace/format-css",
  "version": "1.3.0",
  "description": "Fast, small, zero-config library to format CSS with basic rules.",
  "type": "module",
  "source": "index.js",
  "exports": {
    "require": "./dist/format.cjs",
    "default": "./dist/format.modern.js"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/format.cjs",
  "module": "./dist/format.module.js",
  "unpkg": "./dist/format.umd.js",
  "scripts": {
    "build": "microbundle",
    "test": "uvu",
    "check": "tsc",
    "lint": "oxlint -D perf"
  },
  "devDependencies": {
    "@types/css-tree": "^2.3.4",
    "microbundle": "^0.15.1",
    "oxlint": "^0.2.8",
    "typescript": "^5.3.3",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "css-tree": "^2.3.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "css",
    "stylesheet",
    "formatter",
    "format",
    "print",
    "prettifier",
    "pretty",
    "prettier"
  ]
}